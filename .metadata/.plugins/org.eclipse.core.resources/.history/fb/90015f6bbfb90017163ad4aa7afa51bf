package p1;

public class Demo {

	public static void main(String[] args) {
		String[] names = new String[10];
		names[0] = "Adam";
//		names[1] = "Bill";
		names[2] = "Charlie";
		names[5] = "Zack";
		for (int i = 0; i < names.length; i++) {
			if (names[i] == null) {
				break;
			}
			System.out.println(names[i].length());

		}
	}

}
