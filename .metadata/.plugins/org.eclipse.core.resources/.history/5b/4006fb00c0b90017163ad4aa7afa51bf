package p1;

public class Demo {

	public static void main(String[] args) {
		String[] names = new String[10];
		names[0] = "Adam";
		// names[1] = "Bill";
		names[9] = "Charlie";
		names[5] = "Zack";
		
		// unchecked runtime exception
		for (int i = 0; i < names.length; i++) {
			System.out.println(names[i].length());
		}
		
		System.out.println(names[10]);
	}

}
