package polymorphism;

public class Pet {
	private String name;

	public Pet(String name) {
		super();
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	public void show(Pet pet) {
		System.out.println(pet);
	}

	@Override
	public String toString() {
		return "Pet [name=" + name + "]";
	}

}
